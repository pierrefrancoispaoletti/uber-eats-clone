rd\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n        </Form.Field>\n        <Link to=\"/lostpassword\">Mot de passe perdu ?</Link>\n        <Divider />\n        <Button\n          type=\"submit\"\n          color=\"black\"\n          size=\"large\"\n          content=\"Se connecter\"\n        />\n      </Form>\n      <Link to=\"/register\"><p>Pas encore de compte ? S'enregistrer</p></Link> \n    </Container>\n  );\n};\n\nexport default Login;\n","/var/www/html/ubereats-clone-app/src/store/reducers/auth.js",[],"/var/www/html/ubereats-clone-app/src/containers/Login/Login.container.js",[],"/var/www/html/ubereats-clone-app/src/store/middleware/user.middleware.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Products/Product/Product.js",["174","175","176","177","178"],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Products/Product/ProductModal.js",["179","180"],"/var/www/html/ubereats-clone-app/src/containers/App/App.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Header/Header.container.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/products.js",[],"/var/www/html/ubereats-clone-app/src/containers/ProductModal/ProductModal.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Checkout/Chekout.container.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Checkout/Checkout.js",["181"],"import React, { useState } from \"react\";\nimport { Button, Card, Container, Divider, Header } from \"semantic-ui-react\";\nimport { getCartTotal, uniqueKeyID } from \"../../../utils\";\nimport CartItem from \"./CartItem\";\n\nconst Checkout = ({ cart, user, removeFromCart, emptyCart, updateCartQuantity }) => {\n  return (\n    <Container>\n      <Header as=\"h1\">\n        Bonjour , {`${user.userId.firstName} ${user.userId.lastName}`}\n      </Header>\n      <Header as=\"h3\">Votre Panier</Header>\n      <p>\n        Total : <strong>{getCartTotal(