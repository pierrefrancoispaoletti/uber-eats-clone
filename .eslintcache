(localStorage.user);\n      setUser(localUser);\n    }\n  }, []);\n  useEffect(() => {\n    if (!localStorage.getItem(\"cart\")) {\n      localStorage.setItem(\"cart\", JSON.stringify([]));\n    }\n  }, []);\n  useEffect(() => {\n    if(JSON.parse(localStorage.getItem('cart')) !== []) {\n      const localCart = JSON.parse(localStorage.getItem('cart'))\n      console.log(cart[0])\n      setCart(localCart)\n    }\n  }, []);\n  return (\n    <div className=\"App\">\n      <HeaderMenu visible={visible} setVisible={setVisible} user={user}>\n        <Header visible={visible} setVisible={setVisible} />\n        <Main />\n      </HeaderMenu>\n    </div>\n  );\n}\n\nexport default App;\n","/var/www/html/ubereats-clone-app/src/components/Header/Header.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Main/Main.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/FilteringComponent/FilteringComponent.js",[],"/var/www/html/ubereats-clone-app/src/datas.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/ShopList/ShopList.js",["170"],"import React, { useState } from \"react\";\nimport { Link, useParams } from \"react-router-dom\";\nimport { slugifyUrl } from \"../../../utils\";\nimport ShopItem from \"./ShopItem/ShopItem\";\nimport \"./shoplist.css\";\n\nconst ShopList = ({ shops }) => {\n  const { type } = useParams();\n  const slugifiedType = slugifyUrl(type);\n  const filteredShops = shops\n    ?.map((shop) => {\n      if (slugifyUrl(shop?.type) === slugifiedType) {\n        return shop;\n      } else {\n        return undefined;\n      }\n    })\n    .filter((e) => e !== undefined);\n  return (\n    <div className=\"shopList\">\n      {type && shops !== {} ? (\n        <>\n          <div className=\"shopList__shopType\">\n            <h1>« {type} »</h1>\n            <h2>\n              {filteredShops?.length || 0 }{\" \"}\n              {filteredShops?.length > 1 ? \"Shops\" : \"Shop\"}\n            </h2>\n          </div>\n          {filteredShops?.map(\n            (shop) =>\n              shop.name && (\n                <Link key={shop?._id} to={`/shop/${slugifyUrl(shop?.name)}/${shop._id}`}>\n                  <ShopItem {...shop} />\n                </Link>\n              )\n          )}\n        </>\n      ) : (\n        <>\n          {shops?.map((shop) => (\n            <Link key={shop?._id} to={`/shop/${slugifyUrl(shop?.name)}/${shop?._id}`}>\n              <ShopItem {...shop} />\n            </Link>\n          ))}\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default ShopList;\n","/var/www/html/ubereats-clone-app/src/utils.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Shop.js",["171","172"],"import React from \"react\";\nimport Products from \"./Products/Products\";\nimport { useParams } from \"react-router-dom\";\nimport \"./shop.css\";\nimport Top from \"./Top/Top\";\nimport { useEffect } from \"react/cjs/react.development\";\nimport { useState } from \"react\";\nimport axios from \"axios\";\nimport { uniqueKeyID } from \"../../../utils\";\n\nconst Shop = ({ shops }) => {\n  const { id } = useParams();\n  const [categories, setCategories] = useState([]);\n\n  useEffect(() => {\n    axios.get(`/category/${id}`).then((response) => {\n      setCategories(response.data.data);\n    });\n  }, []);\n\n  const findShopById = shops\n    .map((shop) => {\n      if (shop._id === id) {\n        return shop;\n      }\n    })\n    .find((e) => e !== undefined);\n  return (\n    <div className=\"shop\">\n      <Top {...findShopById} />\n      <Products key={uniqueKeyID()} categories={categories} />\n    </div>\n  );\n};\n\nexport default Shop;\n","/var/www/html/ubereats-clone-app/src/components/Main/ShopList/ShopItem/ShopItem.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/ShopComponents/LikeComponent/LikeComponent.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Top/Top.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Products/Products.js