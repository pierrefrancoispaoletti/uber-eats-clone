d) => {\n    return subCategories\n      .map((subCategory) => {\n        if (subCategory._id === productSubId) {\n          return JSON.parse(subCategory.options);\n        }\n      })\n      .find((s) => s !== undefined);\n  };\n  const productToFind = products\n    ?.map((product) => {\n      if (product?._id === productId) {\n        return product;\n      }\n    })\n    .find((p) => p !== undefined);\n\n  const options = filteredSubCategories(productToFind?.subCategoryId);\n\n  const item = {\n    id: filterFunction(productToFind?.name, o, products)?._id,\n    image: filterFunction(productToFind?.name, o, products)?.urlImage,\n    name: filterFunction(productToFind?.name, o, products)?.name,\n    quantity: quantity,\n    unitPrice: Number(filterFunction(productToFind?.name, o, products)?.price),\n    options: filterFunction(productToFind?.name, o, products)?.options,\n  };\n\n  const handleAddToCart = (e) => {\n    e.preventDefault();\n    if (item?.quantity > 0 && options) {\n      addToCart(item);\n      setQuantity(1);\n    }\n    setIsOpenProductModal(false);\n  };\n\n  let subCatObject = {};\n  const handleChange = (e) => {\n    Object.entries(options)\n      .reverse()\n      .map((option) => {\n        return Object.defineProperty(subCatObject, [option[0]], {\n          value: document.querySelector(`#${option[0]}`).value,\n          enumerable: true,\n        });\n      });\n    setO(subCatObject);\n  };\n  return (\n    <Modal\n      centered\n      closeIcon\n      open={isOpenProductModal}\n      onOpen={() => setIsOpenProductModal(true)}\n      onClose={() => {\n        setIsOpenProductModal(false);\n        setO({ options: [\"sans options\"] });\n      }}\n    >\n      <Modal.Header>{productToFind?.name}</Modal.Header>\n      <Modal.Content image>\n        <Image centered size=\"tiny\" src={productToFind?.urlImage} alt=\"\" />\n      </Modal.Content>\n      <Container textAlign=\"center\">\n        <Modal.Description>\n          <p>{productToFind?.description}</p>\n        </Modal.Description>\n      </Container>\n      <Container textAlign=\"center\">\n        <Modal.Description>\n          <p>\n            Prix de l'article{\" \"}\n            {Number(filterFunction(productToFind?.name, o, products)?.price) ||\n              Number(productToFind?.price)}{\" \"}\n            €\n          </p>\n        </Modal.Description>\n      </Container>\n      <Divider />\n      <Modal.Content>\n        <Form>\n          {options &&\n            Object.entries(options).map(\n              (option) =>\n                option[0] !== \"options\" && (\n                  <>\n                    <Form.Field>{option[0]}</Form.Field>\n                    <select\n                      id={option[0]}\n                      defaultValue=\"\"\n                      onChange={handleChange}\n                    >\n                      <option value=\"\">Veuillez faire votre choix</option>\n                      {option[1].map((value) => (\n                        <option key={value} value={value}>\n                          {value}\n                        </option>\n                      ))}\n                    </select>\n                    <Divider />\n                  </>\n                )\n            )}\n          <Container textAlign=\"center\">\n            <Button\n              onClick={(e) =>\n                quantity <= 0 ? setQuantity(0) : setQuantity(quantity - 1)\n              }\n              circular\n              icon=\"minus\"\n              color=\"red\"\n            />\n            <Button disabled>\n              <strong>{quantity}</strong>\n            </Button>\n            <Button\n              color=\"green\"\n              onClick={(e) => setQuantity(quantity + 1)}\n              circular\n              icon=\"plus\"\n            />\n          </Container>\n          <Divider />\n          <Container textAlign=\"center\">\n            <h2>Total des articles</h2>\n            <p>\n              {Number(filterFunction(productToFind?.name, o, products)?.price) *\n                quantity || productToFind?.price * quantity}{\" \"}\n              <small> € </small>{\" \"}\n            </p>\n          </Container>\n          <Divider />\n          <Container textAlign=\"center\">\n            {filterFunction(productToFind?.name, o, products) !== undefined ? (\n              <Button onClick={handleAddToCart} size=\"large\" color=\"black\">\n                Ajouter au panier {quantity} Articles\n              </Button>\n            ) : (\n              <Button disabled size=\"large\" color=\"red\">\n                Ce produit n'est pas disponible actuelement\n              </Button>\n            )}\n          </Container>\n        </Form>\n      </Modal.Content>\n    </Modal>\n  );\n};\n\nexport default ProductModal;\n","/var/www/html/ubereats-clone-app/src/containers/App/App.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Header/Header.container.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/products.js",[],"/var/www/html/ubereats-clone-app/src/containers/ProductModal/ProductModal.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Checkout/Chekout.container.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Checkout/Checkout.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Checkout/CartItem.js",["240","241"],"import axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { Button, Card, Container, Divider, Image } from \"semantic-ui-react\";\n\nconst CartItem = ({\n  id,\n  name,\n  unitPrice,\n  image,\n  quantity,\n  removeFromCart,\n  updateCartQuantity,\n  options,\n}) => {\n  const [itemQuantity, setItemQuantity] = useState(quantity);\n  useEffect(() => {\n    if (itemQuantity !== quantity) {\n      updateCartQuantity(id, itemQuantity);\n    }\n  }, [itemQuantity]);\n  return (\n    <Card>\n      <Card.Content>\n        <Image floated=\"left\" size=\"small\" src={image} />\n        <h4>{name}</h4>\n        <Card.Meta>{unitPrice * itemQuantity} €</Card.Meta>\n        {Object.keys(options).map((option) =>        \n          <Card.Meta>{option}: {options[option]}</Card.Meta>\n        )}\n        <Divider />\n        <Container textAlign=\"center\" fluid>\n          <Button\n            onClick={(e) =>\n              itemQuantity <= 0\n                ? setItemQuantity(0)\n                : setItemQuantity(itemQuantity - 1)\n            }\n            circular\n            icon=\"minus\"\n            color=\"red\"\n            size=\"tiny\"\n          />\n          <Button disabled>\n            <strong>{itemQuantity}</strong>\n          </Button>\n          <Button\n            color=\"green\"\n            onClick={(e) => setItemQuantity(itemQuantity + 1)}\n            circular\n            icon=\"plus\"\n            size=\"tiny\"\n          />\n        </Container>\n      </Card.Content>\n      <Button\n        onClick={(e) => removeFromCart(id)}\n        color=\"red\"\n        content=\"retirer du panier\"\n        circular\n      />\n    </Card>\n  );\n};\n\nexport default CartItem;\n","/var/www/html/ubereats-clone-app/src/components/Register/Register.js",[],"/var/www/html/ubereats-clone-app/src/store/middleware/shop.Middleware.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/shop.js",[],"/var/www/html/ubereats-clone-app/src/containers/Product/Product.container.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/components.js",[],"/var/www/html/ubereats-clone-app/src/containers/Header/HeaderMenu.container.js",[],"/var/www/html/ubereats-clone-app/src/components/Register/RegisterForm.js",[],"/var/www/html/ubereats-clone-app/src/components/Account/Account.js",[],"/var/www/html/ubereats-clone-app/src/containers/Account/Account.js",[],"/var/www/html/ubereats-clone-app/src/components/Orders/Orders.js",[],"/var/www/html/ubereats-clone-app/src/components/StoreManagement/StoreManagement.js",["242"],"/var/www/html/uber