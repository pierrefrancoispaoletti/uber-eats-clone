e\">\n            <h1>« {type} »</h1>\n            <h2>\n              {filteredShops?.length || 0}{\" \"}\n              {filteredShops?.length > 1 ? \"Shops\" : \"Shop\"}\n            </h2>\n          </div>\n          {!loading ? (\n            filteredShops?.map(\n              (shop) =>\n                shop.name && (\n                  <Link\n                    key={shop?._id}\n                    to={`/shop/${slugifyUrl(shop?.name)}/${shop._id}`}\n                  >\n                    <ShopItem {...shop} />\n                  </Link>\n                )\n            )\n          ) : (\n            <>\n              <Placeholder fluid fluid style={{ height: 300 }}>\n                <Placeholder.Image rectangular />\n              </Placeholder>\n              <Placeholder>\n                <Placeholder.Paragraph>\n                  <Placeholder.Line />\n                  <Placeholder.Line />\n                </Placeholder.Paragraph>\n              </Placeholder>\n              <Placeholder fluid style={{ height: 300 }}>\n                <Placeholder.Image rectangular />\n              </Placeholder>\n              <Placeholder>\n                <Placeholder.Paragraph>\n                  <Placeholder.Line />\n                  <Placeholder.Line />\n                </Placeholder.Paragraph>\n              </Placeholder>\n              <Placeholder fluid style={{ height: 300 }}>\n                <Placeholder.Image rectangular />\n              </Placeholder>\n              <Placeholder>\n                <Placeholder.Paragraph>\n                  <Placeholder.Line />\n                  <Placeholder.Line />\n                </Placeholder.Paragraph>\n              </Placeholder>\n              <Placeholder fluid style={{ height: 300 }}>\n                <Placeholder.Image rectangular />\n              </Placeholder>\n              <Placeholder>\n                <Placeholder.Paragraph>\n                  <Placeholder.Line />\n                  <Placeholder.Line />\