>\n      <Divider />\n      <Form onSubmit={handleSubmit}>\n        <Form.Field label=\"E-mail\" control={Input}>\n          <input value={email} onChange={(e) => setEmail(e.target.value)} />\n        </Form.Field>\n        <Form.Field control={Input} label=\"password\">\n          <input\n            type=\"password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n        </Form.Field>\n        <Link to=\"/lostpassword\">Mot de passe perdu ?</Link>\n        <Divider />\n        <Button\n          type=\"submit\"\n          color=\"black\"\n          size=\"large\"\n          content=\"Se connecter\"\n        />\n      </Form>\n      <Link to=\"/register\"><p>Pas encore de compte ? S'enregistrer</p></Link> \n    </Container>\n  );\n};\n\nexport default Login;\n","/var/www/html/ubereats-clone-app/src/store/reducers/auth.js",[],"/var/www/html/ubereats-clone-app/src/containers/Login/Login.container.js",[],"/var/www/html/ubereats-clone-app/src/store/middleware/user.middleware.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Products/Product/Product.js",["199","200"],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Products/Product/ProductModal.js",["201","202"],"/var/www/html/ubereats-clone-app/src/containers/App/App.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Header/Header.container.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/products.js",[],"/var/www/html/ubereats-clone-app/src/containers/ProductModal/ProductModal.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Checkout/Chekout.container.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Checkout/Checkout.js",["203"],"import React, { useState } from \"react\";\nimport { Button, Card, Container, Divider, Header } from \"semantic-ui-react\";\nimport { getCartTotal, uniqueKeyID } from \"../../../utils\";\nimport CartItem from \"./CartItem\";\n\nconst Checkout = ({ cart, user, removeFromCart, emptyCart, updateCartQuantity }) => {\n  return (\n    <Container>\n      <Header as=\"h1\">\n        Bonjour , {`${user.userId.firstName} ${user.userId.lastName}`}\n      </Header>\n      <Header as=\"h3\">Votre Panier</Header>\n      <p>\n        Total : <strong>{getCartTotal(cart)}</strong> €\n      </p>\n      <Button onClick={e => console.log(getCartTotal(cart))} color=\"green\" content=\"Payer\" />\n      <Divider />\n      <Card.Group stackable itemsPerRow={1}>\n        {cart.map((cartItem) => (\n          <CartItem\n            key={uniqueKeyID()}\n            removeFromCart={removeFromCart}\n            {...cartItem}\n            updateCartQuantity={updateCartQuantity}\n          />\n        ))}\n      </Card.Group>\n    </Container>\n  );\n};\n\nexport default Checkout;\n","/var/www/html/ubereats-clone-app/src/components/Main/Checkout/CartItem.js",["204","205"],"import axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { Button, Card, Container, Divider, Image } from \"semantic-ui-react\";\n\nconst CartItem = ({\n  id,\n  name,\n  unitPrice,\n  image,\n  quantity,\n  removeFromCart,\n  updateCartQuantity,\n  options,\n}) => {\n  const [itemQuantity, setItemQuantity] = useState(quantity);\n  useEffect(() => {\n    if (itemQuantity !== quantity) {\n      updateCartQuantity(id, itemQuantity);\n    }\n  }, [itemQuantity]);\n  return (\n    <Card>\n      <Card.Content>\n        <Image floated=\"left\" size=\"small\" src={image} />\n        <h4>{name}</h4>\n        <Card.Meta>{unitPrice * itemQuantity} €</Card.Meta>\n        <Card.Meta>Couleur: {options?.couleur}</Card.Meta>\n        <Card.Meta>Taille: {options?.taille}</Card.Meta>\n        <Divider />\n        <Container textAlign=\"center\" fluid>\n          <Button\n            onClick={(e) =>\n              itemQuantity <= 0\n                ? setItemQuantity(0)\n                : setItemQuantity(itemQuantity - 1)\n            }\n            circular\n    