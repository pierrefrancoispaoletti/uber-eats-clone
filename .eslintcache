nsole.log(cart[0]);\n      setCart(localCart);\n    }\n  }, []);\n  return (\n    <div className=\"App\">\n      <HeaderMenu user={user}>\n        <Header />\n        <Main />\n      </HeaderMenu>\n    </div>\n  );\n}\n\nexport default App;\n","/var/www/html/ubereats-clone-app/src/components/Header/Header.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Main/Main.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/FilteringComponent/FilteringComponent.js",["192"],"/var/www/html/ubereats-clone-app/src/datas.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/ShopList/ShopList.js",[],"/var/www/html/ubereats-clone-app/src/utils.js",["193"],"import slugify from \"slugify\";\n\nexport const slugifyUrl = (shopName = \"\") => {\n  return slugify(shopName, {\n    replacement: \"-\",\n    lower: true,\n  });\n};\n\nexport const uniqueKeyID = () => {\n  return Math.floor((1 + Math.random()) * 0x10000)\n    .toString(16)\n    .substring(1);\n};\n\nexport const getCartTotal = (cart) =>\n  cart?.reduce(\n    (amount, item) => Number(item.unitPrice * item.quantity) + amount,\n    0\n  );\n\nexport const filterFunction = (\n  objectName = \"\",\n  optionObject = {},\n  items = []\n) => {\n  return items\n    .map((item) => {\n      if (\n        item?.options !== undefined && \n        Object.entries(item?.options)\n          .toString()\n          .localeCompare(Object.entries(optionObject).toString()) === 0 &&\n        item.status === true &&\n        item.name === objectName\n      ) {\n        return item;\n      }\n\n    })\n    .find((e) => e !== undefined);\n};\n","/var/www/html/ubereats-clone-app/src/components/Main/Shop/Shop.js",["194","195"],"/var/www/html/ubereats-clone-app/src/components/Main/ShopList/ShopItem/ShopItem.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/ShopComponents/LikeComponent/LikeComponent.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Shop/Top/Top.js",[],"/var/www/html/ubereats-clone-app/src/c