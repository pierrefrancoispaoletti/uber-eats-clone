  {Number(filterFunction(productToFind?.name, o, products)?.price) ||\n              Number(productToFind?.price)}{\" \"}\n            €\n          </p>\n        </Modal.Description>\n      </Container>\n      <Divider />\n      <Modal.Content>\n        <Form>\n          {options &&\n            Object.entries(options).map((option) => (\n              <>\n                <Form.Field>{option[0]}</Form.Field>\n                <select id={option[0]} defaultValue={0} onChange={handleChange}>\n                  <option value=\"\">Veuillez faire votre choix</option>\n                  {option[1].map((value) => (\n                    <option key={value} value={value}>\n                      {value}\n                    </option>\n                  ))}\n                </select>\n              </>\n            ))}\n\n          <Divider />\n          <Container textAlign=\"center\">\n            <Button\n              onClick={(e) =>\n                quantity <= 0 ? setQuantity(0) : setQuantity(quantity - 1)\n              }\n              circular\n              icon=\"minus\"\n              color=\"red\"\n            />\n            <Button disabled>\n              <strong>{quantity}</strong>\n            </Button>\n            <Button\n              color=\"green\"\n              onClick={(e) => setQuantity(quantity + 1)}\n              circular\n              icon=\"plus\"\n            />\n          </Container>\n          <Divider />\n          <Container textAlign=\"center\">\n            <h2>Total des articles</h2>\n            <p>\n              {Number(filterFunction(productToFind?.name, o, products)?.price) *\n                quantity || productToFind?.price * quantity}{\" \"}\n              <small> € </small>{\" \"}\n            </p>\n          </Container>\n          <Divider />\n          <Container textAlign=\"center\">\n            {filterFunction(productToFind?.name, o, products) ? (\n              <Button onClick={handleAddToCart} size=\"large\" color=\"black\">\n                Ajouter au panier {quantity} Articles\n              </Button>\n            ) : (\n              <Button disabled size=\"large\" color=\"red\">\n                Ce produit n'est pas disponible actuelement\n              </Button>\n            )}\n          </Container>\n        </Form>\n      </Modal.Content>\n    </Modal>\n  );\n};\n\nexport default ProductModal;\n","/var/www/html/ubereats-clone-app/src/containers/App/App.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Header/Header.container.js",[],"/var/www/html/ubereats-clone-app/src/store/reducers/products.js",[],"/var/www/html/ubereats-clone-app/src/containers/ProductModal/ProductModal.container.js",[],"/var/www/html/ubereats-clone-app/src/containers/Checkout/Chekout.container.js",[],"/var/www/html/ubereats-clone-app/src/components/Main/Checkout/Checkout.js",["203"],"import React, { useState } from \"react\";\nimport { Button, Card, Container, Divider, Header } from \"semantic-ui-react\";\nimport { getCartTotal, uniqueKeyID } from \"../../../utils\";\nimport CartItem from \"./CartItem\";\n\nconst Checkout = ({ cart, user, removeFromCart, emptyCart, updateCartQuantity }) => {\n  return (\n    <Container>\n      <Header as=\"h1\">\n        Bonjour , {`${user.userId.firstName} ${user.userId.lastName}`}\n      </Header>\n      <Header as=\"h3\">Votre Panier</Header>\n      <p>\n        Total : <strong>{getCartTotal(cart)}</strong> €\n      </p>\n      <Button onClick={e => console.log(getCartTotal(cart))} color=\"green\" content=\"Payer\" />\n      <Divider />\n      <Card.Group stackable itemsPerRow={1}>\n        {cart.map((cartItem) => (\n          <CartItem\n            key={uniqueKeyID()}\n            removeFromCart={removeFromCart}\n            {...cartItem}\n            updateCartQuantity={updateCartQuantity}\n          />\n        ))}\n      </Card.Group>\n    </Container>\n  );\n};\n\nexport default Checkout;\n","/var/www/html/ubereats-clone-app/src/components/Main/Checkout/CartItem.js",["204","205"],"import axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { Button, Card, Container, Divider, Image } from \"semantic-ui-react\";\n\nconst CartItem = ({\n  id,\n  name,\n  unitPrice,\n  image,\n  quantity,\n  removeFromCart,\n  updateCartQuantity,\n  options,\n}) => {\n  const [itemQuantity, setItemQuantity] = useState(quantity);\n  useEffect(() => {\n    if (itemQuantity !== quantity) {\n      updateCartQuantity(id, itemQuantity);\n    }\n  }, [itemQuantity]);\n  return (\n    <Card>\n      <Card.Content>\n        <Image floated=\"left\" size=\"small\" src={image} />\n        <h4>{name}</h4>\n        <Card.Meta>{unitPrice * itemQuantity} €</Card.Meta>\n        {Object.keys(options).map((option) =>        \n          <Card.Meta>{option}: {options[option]}</Card.Meta>\n        )}\n        <Divider />\n        <Container textAlign=\"center\" fluid>\n          <Button\n            onClick={(e) =>\n              itemQuantity <= 0\n                ? setItemQuantity(0)\n                : setItemQuantity(itemQuantity - 1)\n            }\n            circular\n            icon=\"minus\"\n            color=\"red\"\n            size=\"tiny\"\n          />\n          <Button disabled>\n            <strong>{itemQuantity}</strong>\n          </Button>\n          <Button\n            color=\"green\"\n            onClick={(e) => setItemQuantity(itemQuantity + 1)}\n            circular\n            icon=\"plus\"\n            size=\"tiny\"\n          />\n        </Container>\n      </Card.Content>\n      <Button\n        onClick={(e) => removeFromCart(id)}\n        color=\"red\"\n        content=\"retirer du panier\"\n        circular\n      />\n    </Card>\n  );\n};\n\nexport default CartItem;\n","/var/www/html/ubereats-clone-app/src/components/Register/Register.js",["206","207","208"],"/var/www/html/ubereats-clone-app/src/store/middleware/shop.Middleware.js",[],"/var/www/html/ubereats-clone-app/src/store/reducer